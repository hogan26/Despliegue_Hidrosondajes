<?xml version="1.0" encoding="utf-8"?>

<odoo>
  <data>
    <record id="inches_field_view" model="ir.ui.view">
      <field name="name">inches.field.view</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_only_form_view"/>
      <field name="arch" type="xml">
          <xpath expr="//field[@name='last_update_pricelist_date']" position="before">              
              <field name="inches"/>
          </xpath>                    
      </field>          
    </record>
  </data>
</odoo>